<#@ template language="C#" #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="System.Text" #>
<#@ import namespace="System.Collections.Generic" #>
CREATE PROC <#=this.Table.SP_DELETE()#>
(
    @Id INT
)
AS
BEGIN
    BEGIN TRY
        BEGIN TRAN

            DELETE FROM <#=this.Table.FullName#>
                WHERE [Id] = @Id

        COMMIT TRAN
    END TRY
    BEGIN CATCH
        ROLLBACK TRAN
    END CATCH
END